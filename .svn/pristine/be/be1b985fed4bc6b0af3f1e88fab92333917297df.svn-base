<app-custom-modal-pop-up [basicSetting]="basicSetting" [template]="UserUpdateTemplate"></app-custom-modal-pop-up>
<ng-template #UserUpdateTemplate>
  <app-user-profile [showModelPopup]="showModelPopup"></app-user-profile>
</ng-template>
