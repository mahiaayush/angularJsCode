<app-tab-links [myNavData]="myNavData" [activeLink]="activeLink">
</app-tab-links>
<div class="row">
  <div class="col-md-12 py-2 ">
    <div class="bg-white border p-3 mh320">
      <div class="row">

        <form #myForm="ngForm">

          <div class="col-md-12">
            <!--   <h4 class="subHeading">Metadata Upload</h4>-->
            <p class="small   mt-2 mb-3 ">
              <strong>File size upto 40 MB (XML Only)</strong>
            </p>
            <div class="checkboxBlock">
              <div class="form-check form-check-inline">
                <input class="form-check-input" [(ngModel)]="metadataUploadModel.podXML.value" required type="radio" name="inlineRadioOptions" id="inlineRadio1" value="0">
                <label class="form-check-label" for="inlineRadio1">POD</label>
              </div>
              <div class="form-check form-check-inline">
                <input class="form-check-input" [(ngModel)]="metadataUploadModel.podXML.value" required type="radio" name="inlineRadioOptions" id="inlineRadio2" value="1">
                <label class="form-check-label" for="inlineRadio2">Optimal Inventory</label>
              </div>
              <div class="form-check form-check-inline">
                <input class="form-check-input" [(ngModel)]="metadataUploadModel.podXML.value" required type="radio" name="inlineRadioOptions" id="inlineRadio3" value="2">
                <label class="form-check-label" for="inlineRadio3">XBITS</label>
              </div>
            </div>
            <label for="staticEmail" class="col-form-label mt-2">File </label>
            <div class="input-group  mb-3">
              <div class="custom-file">
                <input required type="file" class="custom-file-input" (change)="fileChange($event)" placeholder="Upload file" name="file" [(ngModel)]="fileInput" accept=".pdf,.doc,.docx,.xml">
                <label class="custom-file-label" for="inputGroupFile02">Choose file</label>
              </div>
            </div>
            <p class="text-right my-4">
              <button [disabled]="!myForm.valid" (click)="upLoadFile()" class="btn btn-primary">Upload</button>

            </p>
            <p class="small  my-3">
              <em>
                <strong>Note</strong> : Please upload file via FTP if file size more than
                <strong> 40 MB</strong>.</em>
            </p>
          </div>

        </form>
      </div>
    </div>
  </div>
</div>
