 /*footer css*/	
footer{padding:20px 0 5px 0}	
.footer-right{color:#848b93;font-size:12px;text-transform:uppercase;}
.copyrite{color:#848b93;font-weight:600;font-size:12px;}
.copyrite	span{color:#c1c1c1}
.graphPreview{text-align:center;padding:10px 0;}
 
 /*header fix on scroll*/
 .fix-header { top:0; width: 100%; z-index: 2 !important;position:fixed;background:#1F2532;-ms-transition: all 0.3s ease-out;   -moz-transition: all 0.3s ease-out; -webkit-transition: all 0.3s ease-out;  -o-transition: all 0.3s ease-out; transition:all 0.3s ease-out;} 
.homeIcon{display:none;} 
.fix-header .homeIcon{display:inline-block;margin-right:20px;padding-top: 5px;}
.fix-header .homeIcon a span{font-size:32px;color:#757b8c;}  
.fix-header .settingIcon{position: inherit;background-color: transparent;padding: 0;float: right;} 
.fix-header .settingIcon a{color:#757b8c;transition: .6s all} 
.fix-header .settingIcon:hover{background-color: transparent; }  
.fix-header .settingIcon:hover a{color:#fff;}  
.smallheaderLink{display:none;}  
.fix-header .smallheaderLink{display:block;float:right;margin-top:2px;margin-right:20px;}  
.fix-header	.settingIcon a i{vertical-align: top;margin-top: 2px;display: block;}
.fix-header .smallheaderLink li.dropdown a{color:#757b8c}
.fix-header .smallheaderLink li.dropdown a:hover,  
.fix-header .smallheaderLink li.dropdown a:active,  
.fix-header .smallheaderLink li.dropdown a:focus{color:#fff;} 
.fix-header .smallheaderLink li.dropdown.show > a{opacity: 1 !important;visibility: visible !important;} 

/* inner page css */
.innerMainHeading{color:#0760a0;font-size:22px;font-weight:600;}
.innerSubHeading{font-size:16px;color:#717788;margin-top:10px;}
.reportDropdown{display:flex;float:right;width:240px;text-align:right;}
.reportDropdown select{max-width:100px;margin-left:12px;}

.custTabs{margin:0;padding:0;}
.custTabs li a:hover{border-bottom:0px;}
.custTabs li a.active, .custTabs li a:hover{border-top:0;border-left:0;border-right:0;color:#e8001e !important;}
.custTabs li a.active span{border-bottom:5px solid #e8001e !important;padding-bottom:8px;}
.tableStyle{width:100%;margin:20px 0;}
.tableStyle tbody tr{margin-top:14px;  }
.nav-tabs .nav-item.show .nav-link, .nav-tabs .nav-link{border-bottom:2px }
.nav-tabs .nav-link:focus{border:none;}
.nav-tabs .nav-item.show .nav-link, .nav-tabs .nav-link.active,  .nav-tabs .nav-link:hover{border:0px;border-bottom:2px solid #098FF1;}
.mainHeading{ margin: 10px 0px 10px 0px;  clear: both; font-size: 25px;  color: #A20101; border-bottom: 1px solid #64abcf; padding: 5px 0;  }
.subHeading {
    margin: 20px 0px;
    font-size: 15px;
