body{font-size:1rem; font-family: 'Open Sans', sans-serif;font-weight:400;background:#f9f9fb;}
p, h1, h2, h3, h4, h5, h6, ul{margin:0;padding:0; animation: mainmenufadein 2s; -moz-animation: mainmenufadein 2s;  -webkit-animation: mainmenufadein 2s; -o-animation: fadein 2s;}
a, a:hover, a:active, a:focus{outline:none;}
a{color:#098ff1;}
.mainBg{background-color:#1f2532}
.text-gray{color: #757b8c;}
.fontsize13{font-size:0.8125rem;}
.custWrapper{width:100%;max-width:1600px;margin:0 auto; }
header{margin-bottom: 25px;}
.logo{padding:10px 0;}
.settingIcon{position: absolute;right: 0;background-color: #0f7acd;text-decoration: none;color: #fff;font-size: 34px;border-radius: 50%;padding: 0px 12px 0px 12px; top:5px;  transition:0.6s ease-in-out;z-index: 2;}
.settingIcon a{ display: block; text-decoration:none;color:#fff;vertical-align: middle;}
.settingIcon a i{vertical-align: middle;}
.settingIcon a:hover{-webkit-animation: fa-spin 2s infinite linear; animation: fa-spin 2s infinite linear;}
/* spin the seeting icon */
@-webkit-keyframes fa-spin {
  0% {
    -webkit-transform: rotate(0deg);
    transform: rotate(0deg);
  }
  100% {
    -webkit-transform: rotate(359deg);
    transform: rotate(359deg);
  }
}
@keyframes fa-spin {
  0% {
    -webkit-transform: rotate(0deg);
    transform: rotate(0deg);
  }
  100% {
    -webkit-transform: rotate(359deg);
    transform: rotate(359deg);
  }
}
 .rounded15{border-radius:15px !important; }
 .ptb{padding-top:10%;padding-bottom:10%;}
.settingIcon:hover{text-decoration:none;background-color:#E6212C;color:#fff;}
.custNav > li{margin-right: 150px;}
.custNav  li:last-child{margin-right:0px;}
.custNav  li a{font-size:18px;}
.custNav  li a i{font-size:20px;font-size: 30px;vertical-align: text-top;}

.mainSubMenu{top:0;min-width: 300px;}
.custNav li:nth-child(2) .mainSubMenu{min-width: 200px;}
.submenuHeading{width:100%;padding:0 10px;color:#0b8ae9;position:relative;font-size:16px;}
.submenuHeading .close{position:absolute;right:10px;}
.sumenuLinks{margin:0;padding:10px;list-style:none;}
.sumenuLinks li{padding:5px 0 7px 16px; border-bottom:1px solid #e6e8ec;line-height: 18px;position:relative;  }
.sumenuLinks li:before{color:#828da1;content:'\e900';position:absolute;left:-4px;top:6px;font-family: 'icomoon' !important;filter: progid: DXImageTransform.Microsoft.BasicImage(rotation=0.5); -webkit-transform: rotate(-90deg); -moz-transform: rotate(-90deg); -ms-transform: rotate(-90deg); -o-transform: rotate(-90deg); transform: rotate(-90deg);font-size: 26px;}
.sumenuLinks li:last-child:before{top:4px;}
.sumenuLinks li a{color:#828da1;font-size:14px;}
.sumenuLinks li a:hover{text-decoration:none;}
.sumenuLinks li:last-child{line-height: normal;border-bottom: 0;padding-top: 4px;}
#navbarSupportedContent .dropdown.show > a{opacity: 0; visibility: hidden;animation: mainmenufadeOut 0.5s; -moz-animation: mainmenufadeOut 0.5s;  -webkit-animation: mainmenufadeOut 0.5s;  -o-animation: mainmenufadeOut 0.5s; }
#navbarSupportedContent .dropdown > a{ animation: mainmenufadein 2s; -moz-animation: mainmenufadein 2s;  -webkit-animation: mainmenufadein 2s; -o-animation: fadein 2s;}
 
 
@keyframes mainmenufadein {
    from {
        opacity:0;
    }
    to {
        opacity:1;
    }
}
@-moz-keyframes mainmenufadein { /* Firefox */
    from {
        opacity:0;
    }
    to {
        opacity:1;
    }
}
@-webkit-keyframes mainmenufadein { /* Safari and Chrome */
    from {
        opacity:0;
    }
    to {
        opacity:1;
    }
}
@-o-keyframes mainmenufadein { /* Opera */
    from {
        opacity:0;
    }
    to {
        opacity: 1;
    }
}


@keyframes mainmenufadeOut {
    from {
        opacity:1;
    }
    to {
        opacity:0;
    }
}
@-moz-keyframes mainmenufadeOut { /* Firefox */
    from {
        opacity:1;
    }
    to {
        opacity:0;
    }
}
@-webkit-keyframes mainmenufadeOut { /* Safari and Chrome */
    from {
        opacity:1;
    }
    to {
        opacity:0;
    }
}
@-o-keyframes mainmenufadeOut { /* Opera */
    from {
        opacity:1;
    }
    to {
        opacity: 0;
    }
}

.sidenav { padding-bottom: 90px;height: 100%; width: 0px; position: fixed; z-index: 3; top: 0;right: 0; background-color: #fff;overflow-x: hidden; transition: 0.5s; -webkit-box-shadow:  0px 0px 4px 0px rgba(0,0,0,0.61); -moz-box-shadow:  0px 0px 4px 0px rgba(0,0,0,0.61); box-shadow:  0px 0px 4px 0px rgba(0,0,0,0.61);}
.sidenavBlock{padding:10px 20px 0 20px;}
.sidemenuBackDrop{ background:rgba(0,0,0,0.5);width:100%;position: fixed;z-index: 3;left: 0;right: 0;top: 0;bottom: 0;}
.buttonBlock{position:absolute;bottom:0;width:89%;text-align:center;border-top:1px solid #9ba4b4;padding:20px 0}
.btnGray{background:#828da1;color:#fff;text-decoration:none;padding:5px 30px;line-height:normal;font-size:16px;}
.btnGray:hover{background:#50586b;color:#fff;text-decoration:none;}
.btnGreen{background:#33a525;color:#fff;text-decoration:none;padding:5px 30px;line-height:normal;font-size:16px;}
.btnGreen:hover{background:#298b1d;color:#fff;text-decoration:none;}
.mainHeading{color:#717788;font-size:18px;margin-bottom:10px;}
.mainHeading a{float:right;font-size:13px;color:#0b8ae9;margin-top:8px;}
.menuSubHeading{background:#0f7acd;font-size:14px;color:#fff;border-radius:5px;padding:5px 8px;font-weight:600;text-transform:uppercase;letter-spacing: 0.02rem;margin-bottom:10px;}
.rightMenuLinks{margin:0 0 30px 0;padding:0;}
.rightMenuLinks > li{margin-top:0px;list-style:none;padding-left:20px;position:relative;border-bottom:1px solid #e6e8ec;padding-bottom:5px;}
.rightMenuLinks > li:after{content:'\e9ce';position:absolute;right:0;top:2px;font-family: 'icomoon' !important;color:#50586b;font-size:16px;display:none;}
.rightMenuLinks > li:hover:after{display:block}
.rightMenuLinks > li:last-child{border-bottom:0;}
.rightMenuLinks > li:before{content:'\e904';position:absolute;left:0;top:2px;font-family: 'icomoon' !important;color:#189e02;font-size:20px}
.rightMenuLinks > li.disabled:before{ color:#c6cbd3; }
.rightMenuLinks > li a{color:#000;font-size:14px;}
.rightMenuLinks > li a:hover{text-decoration:none;}
.slider li {padding:0 20px;width:150px !important;}
.slider li  img{border:1px solid #ccd1d8;position:relative;}
.slider li a{float:left;position:relative;}
.slider li a:hover{text-decoration:none;}
.slider li a i{position:absolute;right:10px;top:10px;color:#189e02;font-size:20px;}
.slider li a i.diable{color:#c6cbd3;}
.slider li a span{display:block;font-size:12px;color:#828da1;text-align:center;font-weight:600;text-transform:uppercase}

.graphBlock{-webkit-box-shadow: 0px 0px 27px 0px rgba(139,139,139,0.13);
-moz-box-shadow: 0px 0px 27px 0px rgba(139,139,139,0.13);
box-shadow: 0px 0px 27px 0px rgba(139,139,139,0.13);background-color:#fff;padding:10px;margin-top:25px;}
.rightMenuLinks li .dropdown-menu{padding:10px;top:0;transform: translate3d(0px, 0, 0px) !important;width:100%;-webkit-box-shadow: 0px 0px 10px 0px rgba(0,0,0,0.2);-moz-box-shadow: 0px 0px 10px 0px rgba(0,0,0,0.2);box-shadow: 0px 0px 10px 0px rgba(0,0,0,0.2);}
.rightnavHeading{color:#0f7acd;font-size:14px;position:relative;padding-left:25px;}
.rightnavHeading button{position:absolute;right:0;top:2px;font-size: 14px;color:#0f7acd;}
.rightnavHeading button:hover, .rightnavHeading button:focus{color:#0f7acd;}
.rightnavHeading:before{content:'\e904';position:absolute;left:0;top:-1px;font-family: 'icomoon' !important;color:#c6cbd3;font-size:20px}
.menuContent{margin:10px 0;padding:10px;background:#efefef;}
.menuContent li{margin-left:15px;line-height:16px;margin-bottom:8px;font-size:12px;}

