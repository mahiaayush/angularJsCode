<div class="container-fluid bg-light">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <nav aria-label="breadcrumb">
          <ol class="breadcrumb">
            <li class="breadcrumb-item">
              Home
            </li>
            <li class="breadcrumb-item active" aria-current="page">Title Overview</li>
          </ol>
        </nav>
      </div>
    </div>
  </div>
</div>
<section class="container-fluid">
  <div class="container ">
    <div class="row">
      <div class="col-md-12 py-2 ">
        <div class="bg-white p-3">

          <div class="row">
            <div class="col-md-12">

              <h4 class="subHeading">Title Overview</h4>

              <form>
                <div class="form-group row">
                  <div class="col-md-6">
                    <label for="isbn" class="col-form-label">ISBN:
                    </label>

                    <input type="text" class="form-control" id="isbn" placeholder="">

                  </div>
                  <div class="col-md-6">
                    <label for="staticEmail" class="col-form-label">Product Sponsor:
                    </label>
                    <input type="checkbox" id="ge">
                    <label for="ge"> GE </label>
                    <input type="checkbox" id="pd" title="PD">
                    <label for="pd"> PD </label>
                    <input type="checkbox" id="gr" title="GR">
                    <label for="gr"> GR </label>
                  </div>

                </div>

                <div class="form-group row">
                  <div class="col-md-6">
                    <label for="title" class=" col-form-label">Title:</label>

                    <input type="text" class="form-control" id="title" placeholder="">

                  </div>
                  <div class="col-md-6">
                    <label for="inputPassword" class=" col-form-label">Product Line: </label>


                    <select class="form-control">
                      <option>4-Accounting</option>
                      <option>5L-HE Imports Under Consideration</option>
                      <option>8-Phys & Social Sci; World Langs</option>
                      <option>A-Wiley International Editions</option>
                    </select>
                  </div>

                </div>
                <div class="form-group row">
                  <div class="col-md-6">
                    <label for="inputAuthor" class="col-form-label">Author:</label>

                    <input type="text" class="form-control" id="inputAuthor" placeholder="">

                  </div>
                  <div class="col-md-6">
                    <label for="inputPassword" class=" col-form-label">Product Process Code: </label>

                    <select class="form-control">
                      <option>A</option>
                      <option>AD</option>
                      <option>AU</option>
                      <option>B</option>
                    </select>

                  </div>

                </div>
                <div class="form-group row">
                  <div class="col-md-6">
                    <label for="inputfrom" class=" col-form-label">From Date: </label>

                    <input type="date" class="form-control" id="inputfrom" placeholder="">

                  </div>
                  <div class="col-md-6">
                    <label for="inputtodate" class=" col-form-label">To Date: </label>

                    <input type="date" class="form-control" id="inputtodate" placeholder="">

                  </div>


                </div>
              </form>
              <p class="text-right mt-5 mb-5">
                <a href="javascript:;" class="btn btn-primary btn-sm" id="btnLogin">Save</a>
                <a href="javascript:;" class="btn btn-secondary btn-sm mr-2" id="btnLogin">Reset</a>
              </p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
