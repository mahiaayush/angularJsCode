<h5>
  Details of ISBN ({{rowData[0].isbn}})</h5>
<div class="row">
  <div class="col-md-12 agGridSize">
    <app-ag-grid [name]="'vendorDetails'" [myGridOptions]="gridOptions" (myGridReady)="doOnGridReadyVendorDetails($event)">
    </app-ag-grid>
  </div>
</div>


<ng-template #history>
  <app-history [rowData]="rowData"></app-history>
</ng-template>
