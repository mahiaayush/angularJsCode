.ag-cell {
  overflow: initial !important;
}

.ag-paging-panel>span:last-child {
  display: none !important;
}

.ag-paging-panel>span:first-child {
  padding: 0;
}

.ag-paging-panel {
  position: absolute;
  float: left;
  padding: 0 !important;
}

.ag-paging-panel {
  font-size: medium;
}
