<app-tab-links [myNavData]="myNavData" [activeLink]="activeLink">
</app-tab-links>

<div class="row">
  <div class="col-md-12 py-2 ">
    <div class="bg-white p-3 mh320 border">
      <div class="row">
        <div class="col-md-12">
          <h4 class="subHeading">User Details</h4>
          <form>
            <div class="form-group row">
              <div class="col-md-4">
                <label for="staticEmail" class="col-form-label">Role
                  <sup class="text-danger">*</sup>
                </label>
                <input type="text" class="form-control" id="inputPassword" placeholder="" [(ngModel)]="createUserModel.role.value" [name]="createUserModel.role.name">
              </div>
              <div class="col-md-4">
                <label for="inputPassword" class=" col-form-label">First Name
                  <sup class="text-danger">*</sup>
                </label>
                <input type="text" class="form-control" id="inputPassword" placeholder="" [(ngModel)]="createUserModel.fName.value" [name]="createUserModel.fName.name">
              </div>
              <div class="col-md-4">
                <label for="inputPassword" class=" col-form-label">Last Name</label>
                <input type="text" class="form-control" id="inputPassword" placeholder="" [(ngModel)]="createUserModel.lName.value" [name]="createUserModel.lName.name">
              </div>
            </div>
            <div class="form-group row">
              <div class="col-md-4">
                <label for="staticEmail" class="col-form-label">Phone
                  <sup class="text-danger">*</sup>
                </label>
                <input type="text" class="form-control" id="inputPassword" placeholder="" [(ngModel)]="createUserModel.phone.value" [name]="createUserModel.phone.name">
              </div>
              <div class="col-md-4">
                <label for="inputPassword" class=" col-form-label">Address </label>
                <input type="text" class="form-control" id="inputPassword" placeholder="" [(ngModel)]="createUserModel.address.value" [name]="createUserModel.address.name">
                <small>(Please write not more than 255 characters)</small>
              </div>
              <div class="col-md-4">
                <label for="inputPassword" class=" col-form-label">City/Town
                  <sup class="text-danger">*</sup>
                </label>
                <input type="text" class="form-control" id="inputPassword" placeholder="" [(ngModel)]="createUserModel.city.value" [name]="createUserModel.city.name">
              </div>
            </div>
            <div class="form-group row">
              <div class="col-md-4">
                <label for="staticEmail" class="col-form-label">Email/Login</label>
                <input type="text" class="form-control" id="inputPassword" placeholder="" [(ngModel)]="createUserModel.email.value" [name]="createUserModel.email.name">
              </div>
              <div class="col-md-4">
                <label for="inputPassword" class=" col-form-label">Password </label>
                <input type="text" class="form-control" id="inputPassword" placeholder="" [(ngModel)]="createUserModel.password.value" [name]="createUserModel.password.name">
              </div>
              <div class="col-md-4">
                <label for="inputPassword1" class=" col-form-label">Confirm Password*</label>
                <input type="text" class="form-control" id="inputPassword1" placeholder="" [(ngModel)]="createUserModel.confirmPassword.value"
                  [name]="createUserModel.confirmPassword.name">
              </div>
            </div>
            <div class="form-group row">
              <div class="col-md-4">
                <label for="inputPassword" class=" col-form-label">State/Province </label>
                <input type="text" class="form-control" id="inputPassword" placeholder="" [(ngModel)]="createUserModel.state.value" [name]="createUserModel.state.name">
              </div>
              <div class="col-md-4">
                <label for="inputPassword" class=" col-form-label">Post/Zip Code </label>
                <input type="text" class="form-control" id="inputPassword" placeholder="" [(ngModel)]="createUserModel.zipCode.value" [name]="createUserModel.zipCode.name">
              </div>
              <div class="col-md-4">
                <label for="staticEmail" class="col-form-label">Country </label>
                <select class="form-control" [(ngModel)]="createUserModel.country.value" [name]="createUserModel.country.name">
                  <option>Choose Country</option>
                  <option>India</option>
                </select>
              </div>
            </div>
            <p class="text-right mt-5 mb-5">
              <button type="submit" class="btn btn-primary btn-sm" id="btnLogin" (click)="createNewUser()">Save</button>
              <button type="reset" class="btn btn-secondary btn-sm mr-2" id="btnLogin">Reset</button>
              <!-- <a href="javascript:;" class="btn btn-secondary btn-sm mr-2" id="btnLogin">Reset</a> -->
            </p>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>
