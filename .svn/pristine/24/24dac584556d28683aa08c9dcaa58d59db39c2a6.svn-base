


export class ObjectUtil {

      asignObjTofirstOne(first: any, ...others: Array<any>) {

            others.forEach((other) => {
                  Object.assign(first, other);
            });
            return first;
      }


      deleteKeys(obj: any, ...keys: Array<string>) {
            keys.forEach((key) => {
                  delete obj[key];
            });
      }




}
